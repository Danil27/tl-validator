{
  "name": "tl-lint",
  "version": "1.0.0",
  "description": "tl file validator",
  "main": "tllint.js",
  "type": "commonjs",
  "scripts": {
    "test": "test",
    "tllint": "node --es-module-specifier-resolution=node ./tools/tllint.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "tl-validator"
  },
  "lint-staged": {
    "*.tl": "npm run tllint"
  },
  "keywords": [
    "tl",
    "tllint",
    "validator"
  ],
  "author": "Danila Krygin",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3"
  }
}